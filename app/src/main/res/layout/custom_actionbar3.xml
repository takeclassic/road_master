<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="60dp"
    android:background="@drawable/actionbar"
    android:padding="10dp"
    android:focusable="true"
    android:focusableInTouchMode="true">

    <ImageView
        android:id="@+id/actionbar3_icon"
        android:src="@drawable/icon_noback"
        android:layout_width="30dp"
        android:layout_height="30dp"/>

    <EditText
        android:id="@+id/actionbar3_text"
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:layout_toRightOf="@id/actionbar3_icon"
        android:background="@drawable/leftround"
        android:layout_marginLeft="10dp"
        android:layout_marginBottom="5dp"
        android:paddingLeft="10dp"
        android:hint="강의실 번호, 이름 등으로 검색"/>

    <ImageButton
        android:id="@+id/actionbar3_search"
        android:src="@drawable/search"
        android:background="@drawable/rightround"
        android:scaleType="centerInside"
        android:padding="7dp"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_alignParentRight="true"/>

    <ImageButton
        android:id="@+id/actionbar3_delete"
        android:layout_toLeftOf="@id/actionbar3_search"
        android:src="@drawable/delete"
        android:scaleType="centerInside"
        android:padding="7dp"
        android:background="#00000000"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:visibility="invisible"
        android:clickable="false" />

</RelativeLayout>